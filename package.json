{
    "name": "symfony",
    "displayName": "symfony",
    "description": "symfony extension",
    "version": "0.0.1",
    "publisher": "shustrik",
    "engines": {
        "vscode": "^1.11.0"
    },
    "categories": [
        "Frameworks"
    ],
   "activationEvents": [
      "onLanguage:php",
      "onLanguage:yaml"
    ],
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "tsc -p ./ && cp ./src/builtInSymbols.json ./out/",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32",
        "yaml-js": "^0.1.5",
        "xml4js": "^0.1.3",
        "xml2js": "^0.4.17",
        "xml-js": "^1.2.2",
        "intelephense": "^0.6.9"
    }
}